@model DogWalkerAgain.Models.Dog

@{
    ViewBag.Title = "Index";
}



<!DOCTYPE html>
<html>
<head>

    <title></title>
    <style>
        * {
            margin: 0;
            padding: 0;
        }

        #map {
            height: 500px;
            width: 100%;
        }
    </style>
</head>
<body>
    <div id="map">

    </div>


    <script>
        function initMap() {
            var location = { lat: 43.03, lng: -87.906 };
            var map = new google.maps.Map(document.getElementById("map"), {
                zoom: 4,
                center: location
            });
        }

    </script>
    <script async defer src="https://maps.googleapis.com/maps/js?key=&callback=initMap"></script>


</body>
</html>
